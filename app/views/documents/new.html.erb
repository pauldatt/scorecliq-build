<div class="score-contain">
      <h1 class="score-header-form"></h1>
      <div class="score-form">
         
        <h2>Upload Documents</h2>
          
         <%= form_for [@scoreboard, @category, @document] do |f| %>
         
         <%= render 'shared/error_messages', object: f.object %>
        
         <%= f.label :file_name %>
         <%= f.text_field :file_name, class: "form-control" %>
        
         <%= f.label :upload_file %>
         <%= f.file_field :file, class: "form-control"  %>
              
         <%= f.submit "Create", :data => {:disable_with => "Creating Event..."}, class: "btn btn-primary" %>
         
         <% end %>
    
      </div>
  </div>
</div>