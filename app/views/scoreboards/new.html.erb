<div class="score-contain">
      <h1 class="score-header-form"></h1>
      <div class="score-form">
         
        <h2>Create New Scoreboard</h2>
          
         <%= form_for(@scoreboard) do |f| %>
         
         <%= render 'shared/error_messages', object: f.object %>
         
         <%= render 'scoreboardform', :f => f %>
         
         <%= f.submit "Create", :data => {:disable_with => "Creating..."}, class: "btn btn-primary" %>
         
         <% end %>
    
      </div>
  </div>
</div>


      

      
     
      
     
      
      

       



  






  

  



